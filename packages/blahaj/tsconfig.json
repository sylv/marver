{
  "extends": "@atlasbot/configs/tsconfig/esm.json",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "declaration": false,
    "paths": {
      "#root/*": ["./src/*"]
    }
  }
}
