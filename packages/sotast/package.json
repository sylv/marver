    "class-validator": "^0.14.1",
    "dedent": "^1.5.1",
    "escape-string-regexp": "^5.0.0",
    "exifreader": "^4.21.0",
    "fastify": "^4.26.1",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^16.8.1",
    "iso-639-1": "^3.1.1",
    "languagedetect": "^2.0.0",
    "lru-cache": "^10.2.0",
    "mercurius": "^13.3.3",
    "merge": "^2.1.1",
