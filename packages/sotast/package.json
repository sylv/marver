  "devDependencies": {
    "@atlasbot/configs": "^10.5.15",
    "@mikro-orm/cli": "^6.1.4",
    "@protobuf-ts/plugin": "^2.9.3",
    "@swc/core": "^1.4.2",
    "@types/bytes": "^3.1.4",
    "@types/fluent-ffmpeg": "^2.1.24",
