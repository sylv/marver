{
  "name": "@marver/sotast",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "watch": "tsup --watch --onSuccess \"node ./dist/main.js\"",
    "build": "tsc --noEmit && tsup",
    "test": "vitest run",
    "mikro-orm": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm",
    "generate": "protoc --ts_out ./src/@generated --ts_opt optimize_speed,use_proto_field_name --proto_path=../../proto ../../proto/*.proto --experimental_allow_proto3_optional && node ./scripts/fix-imports.mjs ./src/@generated"
  },
  "dependencies": {
