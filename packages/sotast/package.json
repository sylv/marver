    "test": "vitest run",
    "mikro-orm": "NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --import @swc-node/register/esm-register\" mikro-orm",
    "generate": "protoc --ts_out ./src/@generated --ts_opt optimize_speed,use_proto_field_name --proto_path=../../proto ../../proto/*.proto --experimental_allow_proto3_optional && node ./scripts/fix-imports.mjs ./src/@generated"
  },
